        <img src="~/WallPaper/image.jpg" alt="Wallpaper" style="height:100vh; width:100%; object-fit:cover;" />
        <div id="box2">
            <div id="text">
                Welcome<br> To Casting Nepal<br>
                <div class="uk-flex">
                    <button class="btn modelhire-btn">Hire Model</button>
                    <button class="btn join-btn">Join</button>
                </div>
            </div>
        </div>
    </div>
</section>


@*Artists Page*@
<div class="uk-margin-small-left uk-margin-small-right" style="margin-top:610px;">
