@*Contact*@
<section id="contact">
    <div class="uk-child-width-expand@s uk-padding" uk-grid style="min-height:50vh;">
        <div style="">
            <h1 style="color: #111; font-size: 5rem;font-weight: 600;text-align: center; margin-bottom:110px;">
                CastingNepal
            </h1>
            <div class="uk-child-width-expand uk-text-center uk-margin-top " uk-grid>
                <div class="uk-width-1-3@s uk-text-center">
                    <div class="social-icon">
                        <a href="https://www.facebook.com/zookti.makeitsimple/" target="_blank" uk-icon="icon: location">
                        </a>
                    </div>
                    <h2>Location</h2>
                    <span>3rd floor, Karki Bhawan, Anamnagar, Kathmandu, Nepal</span>
                </div>
                <div class="uk-width-1-3@s">
                    <div class="social-icon">

                        <a href="https://www.facebook.com/zookti.makeitsimple/" target="_blank" uk-icon="icon: receiver">
                        </a>
                    </div>
                    <h2>Phone</h2>
                    <span>
                        9851187463<br />
                        9828888954
                    </span>
                </div>
                <div class="uk-width-1-3@s">
                    <div class="social-icon">
                        <a href="https://www.facebook.com/zookti.makeitsimple/" target="_blank" uk-icon="mail">
                            @*<i uk-icon="icon: mail ratio: 2"></i>*@
                        </a>
                    </div>
                    <h2>Email</h2>
                    <span>
                        info@zookti.com<br />
                        zooktico@gmail.com
                    </span>
                </div>
            </div>
        </div>
        <div class="uk-width-1-3@s" style="background-color:#ffcc33; padding:36px 45px;  border-radius:7px;">
            <h3 style="text-align: center;font-weight: 600;font-size: 23px;">Contact Us</h3>
            <form class="uk-form-stacked">
                <div class="uk-margin">
                    <label class="uk-form-label label-name" for="form-stacked-text">Name</label>
                    <div class="uk-form-controls input-section">
                        <input class="uk-input" id="form-stacked-text" type="text" placeholder="Name">
                    </div>
                </div>
                <div class="uk-margin">
                    <label class="uk-form-label label-name" for="form-stacked-text">Email</label>
                    <div class="uk-form-controls input-section">
                        <input class="uk-input" id="form-stacked-text" type="text" placeholder="Email">
                    </div>
                </div>
                <div class="uk-margin">
                    <label class="uk-form-label label-name" for="form-stacked-text">Phone</label>
                    <div class="uk-form-controls input-section">
                        <input class="uk-input" id="form-stacked-text" type="number" placeholder="Phone">
                    </div>
                </div>
                <div class="uk-margin">
                    <label class="uk-form-label label-name" for="form-stacked-text">Message</label>
                    <textarea class="uk-textarea textarea-section" rows="3" placeholder="Your Message,Comment,Feedback"></textarea>
                </div>
                <button class="uk-button contact-btn">Send</button>
            </form>
        </div>

    </div>
</section>

