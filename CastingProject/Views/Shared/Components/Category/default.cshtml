@*@model IEnumerable<CastingProject.Models.Cate>*@


@{
    var categories = ViewBag.category as IEnumerable<Category>;
    var selectedCategory = @ViewBag.SelectedCategory;
}


<div class="uk-child-width-expand@s uk-grid-collapse" uk-grid>
    <div class="labelname">
        <label>Category</label>
    </div>
    <div class="selectlist">
        <select name="category"  asp-items="@(new SelectList(categories,"Name", "Name", selectedCategory))">
            <option value="">Select</option>
        </select>
    </div>
</div>

